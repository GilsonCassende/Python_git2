n = int(input("Insira um numero de 1 a 7 : \n"))

if n == 1 :
    print("Domingo")

elif n == 2:
    print("Segunda")   
elif n == 3:
    print("Terça")  
elif n == 4:
    print("quarta")  
elif n == 5:
    print("quinta")  
elif n == 6:
    print("sexta")  

elif n == 7:
    print("sabado")  
else:
    print("Número Invalido")

    



